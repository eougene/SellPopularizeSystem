<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.yd.org.sellpopularizesystem.activity.ProductSubItemDetailActivity">

    <RelativeLayout
        style="@style/MatchWrap"
        android:layout_marginLeft="@dimen/padding_margin_10"
        android:layout_marginRight="@dimen/padding_margin_10"
        android:paddingBottom="@dimen/padding_margin_10"
        android:paddingTop="@dimen/padding_margin_10">

        <TextView
            style="@style/WrapWrap"
            android:layout_gravity="center_vertical"
            android:text="@string/state"
            android:textColor="@color/black"
            android:textSize="@dimen/tv_size_12" />

        <TextView
            android:id="@+id/tvItStatus"
            style="@style/WrapWrap"
            android:layout_alignParentRight="true"
            android:layout_gravity="center_vertical"
            android:text="@string/issaling"
            android:textColor="@color/black"
            android:textSize="@dimen/tv_size_12" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginLeft="@dimen/padding_margin_4"
        android:background="@color/search_edit_bac" />

    <RelativeLayout
        style="@style/MatchWrap"
        android:layout_marginLeft="@dimen/padding_margin_10"
        android:layout_marginRight="@dimen/padding_margin_10"
        android:paddingBottom="@dimen/padding_margin_10"
        android:paddingTop="@dimen/padding_margin_10">

        <TextView
            style="@style/WrapWrap"
            android:layout_gravity="center_vertical"
            android:text="@string/price"
            android:textColor="@color/black"
            android:textSize="@dimen/tv_size_12" />

        <TextView
            android:id="@+id/tvItPrice"
            style="@style/WrapWrap"
            android:layout_alignParentRight="true"
            android:layout_gravity="center_vertical"
            android:text="0.00"
            android:textColor="@color/black"
            android:textSize="@dimen/tv_size_12" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginLeft="@dimen/padding_margin_4"
        android:background="@color/search_edit_bac" />

    <RelativeLayout
        style="@style/MatchWrap"
        android:layout_marginLeft="@dimen/padding_margin_10"
        android:layout_marginRight="@dimen/padding_margin_10"
        android:paddingBottom="@dimen/padding_margin_10"
        android:paddingTop="@dimen/padding_margin_10">

        <TextView
            style="@style/WrapWrap"
            android:layout_gravity="center_vertical"
            android:text="@string/type"
            android:textColor="@color/black"
            android:textSize="@dimen/tv_size_12" />

        <TextView
            android:id="@+id/tvItType"
            style="@style/WrapWrap"
            android:layout_alignParentRight="true"
            android:layout_gravity="center_vertical"
            android:text="@string/unknown"
            android:textColor="@color/black"
            android:textSize="@dimen/tv_size_12" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginLeft="@dimen/padding_margin_4"
        android:background="@color/search_edit_bac" />

    <RelativeLayout
        style="@style/MatchWrap"
        android:layout_marginLeft="@dimen/padding_margin_10"
        android:layout_marginRight="@dimen/padding_margin_10"
        android:paddingBottom="@dimen/padding_margin_10"
        android:paddingTop="@dimen/padding_margin_10">

        <TextView
            style="@style/WrapWrap"
            android:layout_gravity="center_vertical"
            android:text="Lot Number"
            android:textColor="@color/black"
            android:textSize="@dimen/tv_size_12" />

        <TextView
            android:id="@+id/tvItBatchNum"
            style="@style/WrapWrap"
            android:layout_alignParentRight="true"
            android:layout_gravity="center_vertical"
            android:text="@string/theme"
            android:textColor="@color/black"
            android:textSize="@dimen/tv_size_12" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginLeft="@dimen/padding_margin_4"
        android:background="@color/search_edit_bac" />

    <RelativeLayout
        style="@style/MatchWrap"
        android:layout_marginLeft="@dimen/padding_margin_10"
        android:layout_marginRight="@dimen/padding_margin_10"
        android:paddingBottom="@dimen/padding_margin_10"
        android:paddingTop="@dimen/padding_margin_10">

        <TextView
            style="@style/WrapWrap"
            android:layout_gravity="center_vertical"
            android:text="Unit Number"
            android:textColor="@color/black"
            android:textSize="@dimen/tv_size_12" />

        <TextView
            android:id="@+id/tvItUnitNum"
            style="@style/WrapWrap"
            android:layout_alignParentRight="true"
            android:layout_gravity="center_vertical"
            android:text="@string/theme"
            android:textColor="@color/black"
            android:textSize="@dimen/tv_size_12" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginLeft="@dimen/padding_margin_4"
        android:background="@color/search_edit_bac" />

    <RelativeLayout
        style="@style/MatchWrap"
        android:layout_marginLeft="@dimen/padding_margin_10"
        android:layout_marginRight="@dimen/padding_margin_10"
        android:paddingBottom="@dimen/padding_margin_10"
        android:paddingTop="@dimen/padding_margin_10">

        <TextView
            style="@style/WrapWrap"
            android:layout_gravity="center_vertical"
            android:text="@string/floortype"
            android:textColor="@color/black"
            android:textSize="@dimen/tv_size_12" />

        <TextView
            android:id="@+id/tvFloorType"
            style="@style/WrapWrap"
            android:layout_alignParentRight="true"
            android:layout_gravity="center_vertical"
            android:text=""
            android:textColor="@color/black"
            android:textSize="@dimen/tv_size_12" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginLeft="@dimen/padding_margin_4"
        android:background="@color/search_edit_bac" />

    <RelativeLayout
        style="@style/MatchWrap"
        android:layout_marginLeft="@dimen/padding_margin_10"
        android:layout_marginRight="@dimen/padding_margin_10"
        android:paddingBottom="@dimen/padding_margin_10"
        android:paddingTop="@dimen/padding_margin_10">

        <TextView
            style="@style/WrapWrap"
            android:layout_gravity="center_vertical"
            android:text="@string/floor"
            android:textColor="@color/black"
            android:textSize="@dimen/tv_size_12" />

        <TextView
            android:id="@+id/tvItFloor"
            style="@style/WrapWrap"
            android:layout_alignParentRight="true"
            android:layout_gravity="center_vertical"
            android:text="0"
            android:textColor="@color/black"
            android:textSize="@dimen/tv_size_12" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginLeft="@dimen/padding_margin_4"
        android:background="@color/search_edit_bac" />

    <RelativeLayout
        style="@style/MatchWrap"
        android:layout_marginLeft="@dimen/padding_margin_10"
        android:layout_marginRight="@dimen/padding_margin_10"
        android:paddingBottom="@dimen/padding_margin_10"
        android:paddingTop="@dimen/padding_margin_10">

        <TextView
            style="@style/WrapWrap"
            android:layout_gravity="center_vertical"
            android:text="@string/housetype"
            android:textColor="@color/black"
            android:textSize="@dimen/tv_size_12" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_gravity="center_vertical"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/ivItBedroom"
                style="@style/WrapWrap"
                android:layout_gravity="center_vertical"
                android:background="@color/transparent"
                android:src="@mipmap/bedroom" />

            <TextView
                android:id="@+id/tvItBedRoom"
                style="@style/WrapWrap"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="@dimen/padding_margin_4"
                android:text="1"
                android:textColor="@color/black"
                android:textSize="@dimen/tv_size_12" />

            <ImageView
                android:id="@+id/ivItBathroom"
                style="@style/WrapWrap"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="@dimen/padding_margin_6"
                android:src="@mipmap/shower" />

            <TextView
                android:id="@+id/tvItBath"
                style="@style/WrapWrap"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="@dimen/padding_margin_4"
                android:text="12"
                android:textColor="@color/black"
                android:textSize="@dimen/tv_size_12" />

            <ImageView
                android:id="@+id/ivCar"
                style="@style/WrapWrap"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="@dimen/padding_margin_6"
                android:src="@mipmap/car" />

            <TextView
                android:id="@+id/tvItCarSquare"
                style="@style/WrapWrap"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="@dimen/padding_margin_4"
                android:text="1"
                android:textColor="@color/black"
                android:textSize="@dimen/tv_size_12" />
        </LinearLayout>

    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginLeft="@dimen/padding_margin_4"
        android:background="@color/search_edit_bac" />

    <RelativeLayout
        style="@style/MatchWrap"
        android:layout_marginLeft="@dimen/padding_margin_10"
        android:layout_marginRight="@dimen/padding_margin_10"
        android:paddingBottom="@dimen/padding_margin_10"
        android:paddingTop="@dimen/padding_margin_10">

        <TextView
            style="@style/WrapWrap"
            android:layout_gravity="center_vertical"
            android:text="@string/bathroomnum"
            android:textColor="@color/black"
            android:textSize="@dimen/tv_size_12" />

        <TextView
            android:id="@+id/tvItBathNum"
            style="@style/WrapWrap"
            android:layout_alignParentRight="true"
            android:layout_gravity="center_vertical"
            android:text="0.0"
            android:textColor="@color/black"
            android:textSize="@dimen/tv_size_12" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginLeft="@dimen/padding_margin_4"
        android:background="@color/search_edit_bac" />

    <RelativeLayout
        style="@style/MatchWrap"
        android:layout_marginLeft="@dimen/padding_margin_10"
        android:layout_marginRight="@dimen/padding_margin_10"
        android:paddingBottom="@dimen/padding_margin_10"
        android:paddingTop="@dimen/padding_margin_10">

        <TextView
            style="@style/WrapWrap"
            android:layout_gravity="center_vertical"
            android:text="@string/direction"
            android:textColor="@color/black"
            android:textSize="@dimen/tv_size_12" />

        <TextView
            android:id="@+id/tvItDrec"
            style="@style/WrapWrap"
            android:layout_alignParentRight="true"
            android:layout_gravity="center_vertical"
            android:text=""
            android:textColor="@color/black"
            android:textSize="@dimen/tv_size_12" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginLeft="@dimen/padding_margin_4"
        android:background="@color/search_edit_bac" />

    <RelativeLayout
        style="@style/MatchWrap"
        android:layout_marginLeft="@dimen/padding_margin_10"
        android:layout_marginRight="@dimen/padding_margin_10"
        android:paddingBottom="@dimen/padding_margin_10"
        android:paddingTop="@dimen/padding_margin_10">

        <TextView
            style="@style/WrapWrap"
            android:layout_gravity="center_vertical"
            android:text="@string/areanum"
            android:textColor="@color/black"
            android:textSize="@dimen/tv_size_12" />

        <TextView
            android:id="@+id/tvItRoomArea"
            style="@style/WrapWrap"
            android:layout_alignParentRight="true"
            android:layout_gravity="center_vertical"
            android:text="0.00"
            android:textColor="@color/black"
            android:textSize="@dimen/tv_size_12" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginLeft="@dimen/padding_margin_4"
        android:background="@color/search_edit_bac" />

    <RelativeLayout
        style="@style/MatchWrap"
        android:layout_marginLeft="@dimen/padding_margin_10"
        android:layout_marginRight="@dimen/padding_margin_10"
        android:paddingBottom="@dimen/padding_margin_10"
        android:paddingTop="@dimen/padding_margin_10">

        <TextView
            style="@style/WrapWrap"
            android:layout_gravity="center_vertical"
            android:text="@string/outsidearea"
            android:textColor="@color/black"
            android:textSize="@dimen/tv_size_12" />

        <TextView
            android:id="@+id/tvItORoomArea"
            style="@style/WrapWrap"
            android:layout_alignParentRight="true"
            android:layout_gravity="center_vertical"
            android:text="@string/doublezero"
            android:textColor="@color/black"
            android:textSize="@dimen/tv_size_12" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginLeft="@dimen/padding_margin_4"
        android:background="@color/search_edit_bac" />

    <RelativeLayout
        style="@style/MatchWrap"
        android:layout_marginLeft="@dimen/padding_margin_10"
        android:layout_marginRight="@dimen/padding_margin_10"
        android:paddingBottom="@dimen/padding_margin_10"
        android:paddingTop="@dimen/padding_margin_10">

        <TextView
            style="@style/WrapWrap"
            android:layout_gravity="center_vertical"
            android:text="@string/totalarea"
            android:textColor="@color/black"
            android:textSize="@dimen/tv_size_12" />

        <TextView
            android:id="@+id/tvTotalArea"
            style="@style/WrapWrap"
            android:layout_alignParentRight="true"
            android:layout_gravity="center_vertical"
            android:text="0.00"
            android:textColor="@color/black"
            android:textSize="@dimen/tv_size_12" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginLeft="@dimen/padding_margin_4"
        android:background="@color/search_edit_bac" />

    <RelativeLayout
        style="@style/MatchWrap"
        android:layout_marginLeft="@dimen/padding_margin_10"
        android:layout_marginRight="@dimen/padding_margin_10"
        android:paddingBottom="@dimen/padding_margin_10"
        android:paddingTop="@dimen/padding_margin_10">

        <TextView
            style="@style/WrapWrap"
            android:layout_gravity="center_vertical"
            android:text="@string/landarea"
            android:textColor="@color/black"
            android:textSize="@dimen/tv_size_12" />

        <TextView
            android:id="@+id/tvTotalLandArea"
            style="@style/WrapWrap"
            android:layout_alignParentRight="true"
            android:layout_gravity="center_vertical"
            android:text="0.00"
            android:textColor="@color/black"
            android:textSize="@dimen/tv_size_12" />
    </RelativeLayout>
</LinearLayout>
